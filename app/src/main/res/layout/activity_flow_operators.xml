<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".view.FlowOperatorsActivity">


  <Button
      android:id="@+id/btn_aync_two_API_calls"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Multiple API calls with Async" />

  <Button
      android:id="@+id/btn_withContext_two_API_calls"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Multiple API calls with withContext" />
  <Button
      android:id="@+id/btn_with_operators"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Flow builder operators" />

  </LinearLayout>